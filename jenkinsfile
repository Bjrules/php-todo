    agent any

    stages{
        stage("Git Checkout"){
            steps{
                git branch: 'project20-jenkins-Docker', credentialsId: 'Github', url: 'https://github.com/Bjrules/php-todo.git'
            }
        }
    
        stage("West Wood"){
            steps{
                echo "West Wood"
            }
        }

        stage("Banjo"){
            steps{
                echo "Banjo Babade"
            }
        }

        stage("Banke babade"){
            steps{
                echo "Banke"
            }
        }

        stage("David babson"){
            steps{
                echo "Hello World"
            }
        }
    }
}